use_frameworks!

target "demo" do
# Begin Podfile - /Users/williamwheeler/sandbox/nativescript/nativescript-auth0/demo/node_modules/nativescript-auth0/platforms/ios/Podfile 
 platform :ios, '9.0'

pod "Lock", "~> 2.2"
pod "Auth0", "~> 1.5"

post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['SWIFT_VERSION'] = '3.0'
    end
  end
end 
 # End Podfile 

end